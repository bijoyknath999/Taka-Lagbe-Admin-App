<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="5dp"
		android:background="#FFFFFF"
		android:orientation="horizontal"
		android:padding="10dp">
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:gravity="center_vertical"
			android:orientation="vertical"
			android:layout_weight="1">
			<LinearLayout
				android:id="@+id/linear4"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<de.hdodenhof.circleimageview.CircleImageView
					android:id="@+id/circleimageview1"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:layout_marginLeft="5dp"
					android:layout_marginTop="5dp"
					android:layout_marginBottom="5dp"
					android:src="@drawable/logo_4"
					app:civ_border_width="0dp"
					app:civ_border_color="#008DCD"
					app:civ_circle_background_color="#FFFFFF"
					app:civ_border_overlay="true" />
				<LinearLayout
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:orientation="vertical">

					<TextView
						android:id="@+id/textview1"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:layout_marginRight="5dp"
						android:textSize="18sp"
						android:textColor="#000000" />
					<TextView
						android:id="@+id/textview2"
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:layout_marginRight="5dp"
						android:layout_marginTop="2dp"
						android:textSize="14sp"
						android:textColor="#000000" />
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:layout_marginLeft="10dp"
						android:layout_marginTop="5dp">

						<TextView
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginRight="5dp"
							android:text="Total Click :"
							android:textSize="14sp"
							android:textColor="#000000" />

						<TextView
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:id="@+id/users_total_click_txt"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>

					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:layout_marginLeft="10dp"
						android:layout_marginTop="2dp">

						<TextView
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginRight="5dp"
							android:text="Applovin Revenue :"
							android:textSize="14sp"
							android:textColor="#000000" />

						<TextView
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:id="@+id/users_applovin_revenue_txt"
							android:textSize="14sp"
							android:textColor="#000000" />
					</LinearLayout>
					<TextView
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Applovin ➔"
						android:layout_marginLeft="10dp"
						android:layout_marginTop="5dp"
						android:textSize="14sp"
						android:textColor="#000000" />
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:layout_marginLeft="20dp"
						android:layout_marginTop="2dp">
						<TextView
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:id="@+id/users_applovin_reward_txt"
							android:textSize="14sp"
							android:text=""
							android:textColor="#000000"
							android:layout_marginRight="10dp"/>
						<TextView
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:id="@+id/users_applovin_interstitial_txt"
							android:textSize="14sp"
							android:textColor="#000000"/>
					</LinearLayout>
					<TextView
						android:focusable="false"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="StarApp ➔"
						android:layout_marginLeft="10dp"
						android:layout_marginTop="2dp"
						android:textSize="14sp"
						android:textColor="#000000" />
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:layout_marginLeft="20dp"
						android:layout_marginTop="2dp">
						<TextView
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:id="@+id/users_starapp_reward_txt"
							android:textSize="14sp"
							android:textColor="#000000"
							android:layout_marginRight="10dp"/>
						<TextView
							android:focusable="false"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:id="@+id/users_starapp_interstitial_txt"
							android:textSize="14sp"
							android:textColor="#000000"/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear3"
			android:layout_width="wrap_content"
			android:layout_height="match_parent"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview1"
				android:focusable="false"
				android:layout_width="35dp"
				android:layout_height="35dp"
				android:layout_margin="5dp"
				android:src="@drawable/logo_2"
				android:scaleType="fitCenter" />
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
